{
  "log": {
    "loglevel": "info"
  },
  "inbounds": [
    {
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "GENERATE_CLIENT_UUID_HERE",
            "flow": "xtls-rprx-vision"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "dest": "www.microsoft.com:443",
          "xver": 0,
          "serverNames": [
            "www.microsoft.com"
          ],
          "privateKey": "GENERATE_PRIVATE_KEY_HERE",
          "shortIds": [
            "GENERATE_SHORT_ID_HERE"
          ]
        }
      },
      "quantum": {
        "enabled": true,
        "bb84": {
          "strength": 256
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "EXIT_VPS_IP_HERE",
            "port": 443,
            "users": [
              {
                "id": "CHAIN_UUID_HERE",
                "flow": "xtls-rprx-vision",
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "fingerprint": "chrome",
          "serverName": "www.cloudflare.com",
          "publicKey": "EXIT_VPS_PUBLIC_KEY_HERE",
          "shortId": "EXIT_VPS_SHORT_ID_HERE"
        }
      },
      "mux": {
        "enabled": true,
        "concurrency": 8
      }
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "outboundTag": "proxy"
      }
    ]
  },
  "adaptiveMasking": {
    "enabled": true,
    "profile": "https",
    "ipSidr": {
      "enabled": true,
      "whitelist": "/etc/quantum-vpn/russian_ips.txt"
    }
  }
}
